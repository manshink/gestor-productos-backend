<h1>Product List</h1>
<div class="row">
    {{#each products}}
    <div class="col-md-4 mb-4">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text">{{this.description}}</p>
                <p class="card-text">Price: ${{this.price}}</p>
                <p class="card-text">Stock: {{this.stock}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div> 